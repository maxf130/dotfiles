:let mapleader = "-"
:let maplocalleader = "\\"

set nocompatible              " be iMproved, required
filetype off                  " required
filetype plugin indent on    " required

" Random look and feel stuff
set t_Co=256
syntax on
set background=dark
set expandtab
set shiftwidth=2
set softtabstop=2
set number

" Color 80th column
highlight ColorColumn ctermbg=237
set colorcolumn=80
